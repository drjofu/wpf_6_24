<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="Firmenfarbe1">#FFF92121</Color>
  <Color x:Key="Firmenfarbe2">#FFFFF852</Color>
  <SolidColorBrush x:Key="Füllung1" Color="{DynamicResource Firmenfarbe1}"/>
  <SolidColorBrush x:Key="Füllung2" Color="{DynamicResource Firmenfarbe2}"/>
  <LinearGradientBrush x:Key="Füllung3" EndPoint="0.5,1" StartPoint="0.5,0">
    <GradientStop Color="{DynamicResource Firmenfarbe2}"/>
    <GradientStop Color="{DynamicResource Firmenfarbe1}" Offset="1"/>
  </LinearGradientBrush>

</ResourceDictionary>